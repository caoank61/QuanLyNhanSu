@model QuanLyNhanSu.Models.PhongBan

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Chi tiết phòng ban</h2>

<div>
    <h4>Thông tin phòng ban</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Tên phòng ban
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenPhongBan)
        </dd>

        <dt>
           Số điện thoại
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SDT)
        </dd>

    </dl>
</div>
<div class="form-group row">
    <div class="col-md-2">
        <a href="@Url.Action("Index","PhongBan")" class="btn btn-primary">Trở lại</a>
    </div>
</div>