@model QuanLyNhanSu.Models.TrinhDoHocVan

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Xóa trình độ học vấn</h2>

<h3>Bạn có muốn xoa trình độ học vấn "@Model.TrinhDo" không?</h3>
<div>
    <h4>TrinhDoHocVan</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Trình độ học vấn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TrinhDo)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

<div class="form-group row" style="padding-top:10px;padding-left:10px">
    <input type="submit" value="Xóa" class="btn btn-success" />
    <div class="col-md-2">
        <a href="@Url.Action("Index","TrinhDoHocVan")" class="btn btn-primary">Trở lại</a>
    </div>
</div>
    }
</div>
